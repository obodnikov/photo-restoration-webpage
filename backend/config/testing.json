{
  "application": {
    "name": "Photo Restoration API - Test",
    "version": "1.0.0-test",
    "debug": true,
    "log_level": "DEBUG"
  },
  "server": {
    "host": "0.0.0.0",
    "port": 8000,
    "workers": 1
  },
  "cors": {
    "origins": ["http://localhost:3000", "http://testserver"]
  },
  "security": {
    "algorithm": "HS256",
    "access_token_expire_minutes": 1440,
    "remember_me_expire_days": 7
  },
  "api_providers": {
    "huggingface": {
      "api_url": "https://api-inference.huggingface.co/models",
      "timeout_seconds": 60
    },
    "replicate": {
      "timeout_seconds": 120
    }
  },
  "models": [
    {
      "id": "swin2sr-2x",
      "name": "Swin2SR 2x Upscale",
      "model": "caidas/swin2SR-classical-sr-x2-64",
      "provider": "huggingface",
      "category": "upscale",
      "description": "Fast 2x upscaling",
      "enabled": true,
      "parameters": {"scale": 2},
      "tags": ["upscale"],
      "version": "1.0"
    },
    {
      "id": "swin2sr-4x",
      "name": "Swin2SR 4x Upscale",
      "model": "caidas/swin2SR-classical-sr-x4-64",
      "provider": "huggingface",
      "category": "upscale",
      "description": "Fast 4x upscaling",
      "enabled": true,
      "parameters": {"scale": 4},
      "tags": ["upscale"],
      "version": "1.0"
    },
    {
      "id": "qwen-edit",
      "name": "Qwen Image Enhance",
      "model": "Qwen/Qwen-Image-Edit-2509",
      "provider": "huggingface",
      "category": "enhance",
      "description": "AI-powered enhancement and restoration",
      "enabled": true,
      "parameters": {"prompt": "enhance details, remove noise and artifacts"},
      "tags": ["enhance"],
      "version": "1.0"
    }
  ],
  "models_api": {
    "require_auth": false,
    "cache_ttl_seconds": 0
  },
  "database": {
    "url": "sqlite+aiosqlite:///:memory:",
    "echo_sql": false
  },
  "file_storage": {
    "upload_dir": "./test_data/uploads",
    "processed_dir": "./test_data/processed",
    "max_upload_size_mb": 10,
    "allowed_extensions": [".jpg", ".jpeg", ".png"],
    "image_quality": 85
  },
  "session": {
    "cleanup_hours": 24,
    "cleanup_interval_hours": 6,
    "max_age_hours": 48
  },
  "processing": {
    "max_concurrent_uploads_per_session": 3,
    "queue_size": 10
  }
}
